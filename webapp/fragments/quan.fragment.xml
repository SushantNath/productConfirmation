<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:m="sap.m"
	xmlns:core="sap.ui.core" >
	<Dialog
		contentHeight="90%"
		contentWidth="55%"
		showHeader="false">
		
			    <Toolbar height="3rem">
				<m:OverflowToolbar>
					<m:Title text="{i18n>Dec}"/>
